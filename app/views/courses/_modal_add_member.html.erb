<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h4 class="modal-title"><%= t "title_obj.member" %></h4>
    </div>
    <div class="modal-body">
      <%= render partial: "common/user_not_exit_in_course", collection: remain_users, as: :user %>
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-default" data-dismiss="modal"><%= t "btn.btn_close" %></button>
      <button type="button" class="btn btn-primary add-member"><%= t "btn.btn_add_member" %></button>
    </div>
  </div>
</div>
